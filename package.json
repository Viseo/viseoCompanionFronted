{
  "name": "VISEOCompanion_ClientHTML",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-jest": "18.0.0",
    "babel-loader": "^6.4.0",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "chai": "^3.5.0",
    "enzyme": "^2.7.1",
    "ignore-styles": "^5.0.1",
    "isomorphic-fetch": "^2.2.1",
    "istanbul": "^1.0.0-alpha.2",
    "jest": "18.1.0",
    "jsdom": "^9.12.0",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.0",
    "react-script": "^2.0.5",
    "react-scripts": "^0.9.5",
    "react-test-renderer": "~15.4.0",
    "sinon": "^2.0.0-pre.6"
  },
  "dependencies": {
    "istanbul": "^0.4.5",
    "moment": "^2.17.1",
    "muicss": "^0.9.9-rc2",
    "react": "^15.4.2",
    "react-datepicker": "^0.41.1",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "mocha --compilers js:babel-core/register --opts mocha.opts src/**/*.spec.js",
    "testistanbul": "babel-node node_modules/.bin/istanbul cover node_modules/mocha/bin/_mocha -- --opts mocha.opts src/**/*.spec.js",
    "Test+Coverage": "nyc -all -e .jsx npm test",
    "CoverageReport": "nyc report"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  },
  "jest": {
    "preset": "react"
  },
  "nyc": {
    "include": [
      "src/**/*.js",
      "src/*.js"
    ],
    "exclude": [
      "src/__tests__/*.spec.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "extension": [
      ".js"
    ],
    "cache": true,
    "all": true,
    "check-coverage": true,
    "report-dir": "./coverage"
  }
}
